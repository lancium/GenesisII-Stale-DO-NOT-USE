<?xml version="1.0" encoding="utf-8" ?>
<environment-export
	xmlns="http://vcgr.cs.virginia.edu/GenesisII/bes/environment-export">
<!--  If we wanted to have a constant valued variable, this is how we
      do it.
	<environment-variable name="EDITOR">
		<constant-value value="/usr/bin/vim"/>
	</environment-variable>
-->

	<environment-variable name="JAVA_HOME">
		<dynamic-value class="edu.virginia.vcgr.genii.container.bes.envvarexp.JavaHomeProvider"/>
	</environment-variable>

	<environment-variable name="GENII_INSTALL_DIR">
		<dynamic-value class="edu.virginia.vcgr.genii.container.bes.envvarexp.GeniiInstallDirProvider"/>
	</environment-variable>
</environment-export>
