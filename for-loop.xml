<?xml version="1.0" encoding="utf-8" ?>
<gsh:script
	xmlns:gsh="http://vcgr.cs.virginia.edu/genii/xsh/script"
	xmlns:geniix="http://vcgr.cs.virginia.edu/genii/xsh/grid">

	<gsh:for param-name="COUNT" initial-value="0" exclusive-limit="1000"
		increment-value="1">
		<gsh:echo message="Running [${COUNT}]."/>
		<geniix:run>
			<gsh:param>--jsdl=example-jsdl/example-posix-jsdl.xml</gsh:param>
			<gsh:param>/bes-containers/BootstrapContainer</gsh:param>
		</geniix:run>
	</gsh:for>
</gsh:script>
