<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jsdl:JobDefinition xmlns:jsdl-posix="http://schemas.ggf.org/jsdl/2005/11/jsdl-posix" xmlns:wss="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd" xmlns:jsdl-hpcpa="http://schemas.ggf.org/jsdl/2006/07/jsdl-hpcpa" xmlns:hpcfse-ac="http://schemas.ogf.org/hpcp/2007/11/ac" xmlns:jsdl-spmd="http://schemas.ogf.org/jsdl/2007/02/jsdl-spmd" xmlns:jsdl-sweep="http://schemas.ogf.org/jsdl/2009/03/sweep" xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl" xmlns:ns8="http://vcgr.cs.virginia.edu/jsdl/genii" xmlns:ns9="http://schemas.ogf.org/jsdl/2009/03/sweep/functions">
    <jsdl:JobDescription>
        <jsdl:JobIdentification>
            <jsdl:JobName>directory staging test</jsdl:JobName>
        </jsdl:JobIdentification>
        <jsdl:Application>
            <jsdl-posix:POSIXApplication>
                <jsdl-posix:Executable>job_scripts/build_code.sh</jsdl-posix:Executable>
                <jsdl-posix:Argument>cromp_cakes</jsdl-posix:Argument>
                <jsdl-posix:Argument>murphy</jsdl-posix:Argument>
                <jsdl-posix:Output>out</jsdl-posix:Output>
                <jsdl-posix:Error>err</jsdl-posix:Error>
            </jsdl-posix:POSIXApplication>
        </jsdl:Application>
        <jsdl:DataStaging>
            <jsdl:FileName>job_scripts</jsdl:FileName>
            <jsdl:CreationFlag>overwrite</jsdl:CreationFlag>
            <jsdl:DeleteOnTermination>true</jsdl:DeleteOnTermination>
            <jsdl:HandleAsArchive>false</jsdl:HandleAsArchive>
            <jsdl:Source>
                <jsdl:URI>rns:PATH/job_scripts</jsdl:URI>
            </jsdl:Source>
        </jsdl:DataStaging>
        <jsdl:DataStaging>
            <jsdl:FileName>cromp_cakes</jsdl:FileName>
            <jsdl:CreationFlag>overwrite</jsdl:CreationFlag>
            <jsdl:DeleteOnTermination>true</jsdl:DeleteOnTermination>
            <jsdl:HandleAsArchive>false</jsdl:HandleAsArchive>
            <jsdl:Source>
                <jsdl:URI>rns:PATH/vi-source</jsdl:URI>
            </jsdl:Source>
        </jsdl:DataStaging>
        <jsdl:DataStaging>
            <jsdl:FileName>out</jsdl:FileName>
            <jsdl:CreationFlag>overwrite</jsdl:CreationFlag>
            <jsdl:DeleteOnTermination>true</jsdl:DeleteOnTermination>
            <jsdl:HandleAsArchive>false</jsdl:HandleAsArchive>
            <jsdl:Target>
                <jsdl:URI>rns:PATH/build_results.txt</jsdl:URI>
            </jsdl:Target>
        </jsdl:DataStaging>
        <jsdl:DataStaging>
            <jsdl:FileName>murphy</jsdl:FileName>
            <jsdl:CreationFlag>overwrite</jsdl:CreationFlag>
            <jsdl:DeleteOnTermination>true</jsdl:DeleteOnTermination>
            <jsdl:HandleAsArchive>false</jsdl:HandleAsArchive>
            <jsdl:Target>
                <jsdl:URI>rns:PATH/murphy</jsdl:URI>
            </jsdl:Target>
        </jsdl:DataStaging>
        <jsdl:DataStaging>
            <jsdl:FileName>err</jsdl:FileName>
            <jsdl:CreationFlag>overwrite</jsdl:CreationFlag>
            <jsdl:DeleteOnTermination>true</jsdl:DeleteOnTermination>
            <jsdl:HandleAsArchive>false</jsdl:HandleAsArchive>
            <jsdl:Target>
                <jsdl:URI>rns:PATH/build_errors.txt</jsdl:URI>
            </jsdl:Target>
        </jsdl:DataStaging>
    </jsdl:JobDescription>
</jsdl:JobDefinition>
