#!/bin/bash

# Loads GFFS variables for this particular installation location.

export GENII_INSTALL_DIR="${installer:sys.installationDir}"

if [ "${TERM}" != "dumb" -a -z "$PBS_ENVIRONMENT" ]; then
  source $GENII_INSTALL_DIR/xsede_tools/prepare_tools.sh $GENII_INSTALL_DIR/xsede_tools/prepare_tools.sh 
fi


