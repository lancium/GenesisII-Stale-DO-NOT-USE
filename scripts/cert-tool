#!/bin/sh

_CT_GENII_INSTALL_DIR="%{INSTALL_PATH}"
_CT_LOCAL_JAVA_DIR="$_CT_GENII_INSTALL_DIR/Java/linux-i586/jre"

"$_CT_LOCAL_JAVA_DIR/bin/java" -classpath "%{INSTALL_PATH}/lib:%{INSTALL_PATH}/ApplicationWatcher/app-watcher.jar" "-Dlog4j.configuration=%{LOG4JCONFIG}" "-Djava.library.path=%{INSTALL_PATH}/jni-lib" edu.virginia.vcgr.appwatcher.ApplicationLauncher "%{INSTALL_PATH}/ApplicationWatcher/genii-certtool-application.properties" "$@"
