<?xml version="1.0" encoding="UTF-8"?>
<Sweep 
	xmlns="http://schemas.ogf.org/jsdl/2009/03/sweep"
    xmlns:sweep="http://schemas.ogf.org/jsdl/2009/03/sweep"
    xmlns:sweepfunc="http://schemas.ogf.org/jsdl/2009/03/sweep/functions">
    <Assignment>
    	<DocumentNode>
    		<NamespaceBinding
    			ns="http://schemas.ggf.org/jsdl/2005/11/jsdl-posix"
    			prefix="jsdl-posix"/>
    		<Match>
    			substring(/*//jsdl-posix:Argument[2], 4, 3)
    		</Match>
    	</DocumentNode>
		<sweepfunc:Values>
			<sweepfunc:Value>001</sweepfunc:Value>
			<sweepfunc:Value>002</sweepfunc:Value>
			<sweepfunc:Value>003</sweepfunc:Value>
			<sweepfunc:Value>004</sweepfunc:Value>
			<sweepfunc:Value>005</sweepfunc:Value>
		</sweepfunc:Values>
    </Assignment>
    
    <Sweep>
    	<Assignment>
    		<DocumentNode>
	    		<NamespaceBinding
	    			ns="http://schemas.ggf.org/jsdl/2005/11/jsdl-posix"
	    			prefix="jsdl-posix"/>
	    		<Match>
	    			substring(/*//jsdl-posix:Argument[2], 8, 2)
	    		</Match>
	    	</DocumentNode>
	    	<sweepfunc:Values>
	    		<sweepfunc:Value>Mark</sweepfunc:Value>
	    	</sweepfunc:Values>
	    </Assignment>
	</Sweep>
</Sweep>
